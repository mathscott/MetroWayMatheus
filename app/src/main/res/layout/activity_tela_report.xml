<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TelaReport"
    tools:layout_editor_absoluteY="25dp">


    <com.example.matheus.metrowaymatheus.CheckableImageButton
        android:id="@+id/botaoLentidao"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="105dp"
        android:layout_marginEnd="25dp"
        android:adjustViewBounds="true"
        android:background="@drawable/colorir_botoes"
        android:scaleType="fitXY"
        android:src="@drawable/slow"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/botaoArCondicionado"
        app:layout_constraintHorizontal_bias="0.57"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.example.matheus.metrowaymatheus.CheckableImageButton
        android:id="@+id/botaoAssedio"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="25dp"
        android:adjustViewBounds="true"
        android:background="@drawable/colorir_botoes"
        android:scaleType="fitXY"
        android:src="@drawable/sexual_harassment"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/botaoLimpeza"
        app:layout_constraintStart_toStartOf="parent" />

    <com.example.matheus.metrowaymatheus.CheckableImageButton
        android:id="@+id/botaoLimpeza"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginEnd="39dp"
        android:adjustViewBounds="true"
        android:background="@drawable/colorir_botoes"
        android:scaleType="fitXY"
        android:src="@drawable/clean"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/botaoSeguranca" />

    <com.example.matheus.metrowaymatheus.CheckableImageButton
        android:id="@+id/botaoSeguranca"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginEnd="36dp"
        android:adjustViewBounds="true"
        android:background="@drawable/colorir_botoes"
        android:scaleType="fitXY"
        android:src="@drawable/shield"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent" />

    <com.example.matheus.metrowaymatheus.CheckableImageButton
        android:id="@+id/botaoArCondicionado"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="39dp"
        android:adjustViewBounds="true"
        android:background="@drawable/colorir_botoes"
        android:scaleType="fitXY"
        android:src="@drawable/ac"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/botaoLotacao"
        app:layout_constraintStart_toEndOf="@+id/botaoLentidao"
        tools:src="@drawable/ac" />

    <com.example.matheus.metrowaymatheus.CheckableImageButton
        android:id="@+id/botaoLotacao"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginTop="105dp"
        android:layout_marginEnd="36dp"
        android:adjustViewBounds="true"
        android:background="@drawable/colorir_botoes"
        android:scaleType="fitXY"
        android:src="@drawable/group"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botaoOk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="148dp"
        android:layout_marginEnd="148dp"
        android:layout_marginBottom="56dp"
        android:text="ok"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="87dp"
        android:layout_height="48dp"
        android:layout_marginStart="35dp"
        android:layout_marginBottom="21dp"
        android:text="Lentidão"
        app:layout_constraintBottom_toTopOf="@+id/botaoAssedio"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="87dp"
        android:layout_height="48dp"
        android:layout_marginStart="26dp"
        android:layout_marginBottom="21dp"
        android:text="Ar Condicionado"
        app:layout_constraintBottom_toTopOf="@+id/botaoLimpeza"
        app:layout_constraintStart_toEndOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="87dp"
        android:layout_height="48dp"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="21dp"
        android:text="Lotação"
        app:layout_constraintBottom_toTopOf="@+id/botaoSeguranca"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="87dp"
        android:layout_height="48dp"
        android:layout_marginStart="35dp"
        android:layout_marginBottom="169dp"
        android:text="Assédio Sexual"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="87dp"
        android:layout_height="48dp"
        android:layout_marginStart="35dp"
        android:layout_marginBottom="65dp"
        android:text="Limpeza"
        app:layout_constraintBottom_toTopOf="@+id/botaoOk"
        app:layout_constraintStart_toEndOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="87dp"
        android:layout_height="48dp"
        android:layout_marginStart="47dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="169dp"
        android:text="Segurança"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="324dp"
        android:layout_height="34dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="39dp"
        android:text="Reportar informações do metrô"
        android:textColor="@color/purple"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/botaoLentidao"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</android.support.constraint.ConstraintLayout>